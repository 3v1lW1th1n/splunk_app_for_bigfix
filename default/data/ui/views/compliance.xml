<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Compliance Overview</label>
  <row>
    <html>
      <h1>Compliance Overview</h1>
      <p>
        For filterable details, head over to the <a href="compliance_details">detailed compliance view</a>.
      </p>
    </html>
  </row>
  <row>
    <chart>
      <searchName>compliance by server</searchName>
      <title>Total Compliance By Server</title>
      <option name="charting.chart">column</option>
      <option name="charting.axisY.maximumNumber">100</option>
    </chart>
    <chart>
      <searchName>compliance by severity</searchName>
      <title>Compliance By Fixlet Severity</title>
      <option name="charting.chart">bar</option>
      <option name="charting.axisX.maximumNumber">100</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>compliance over time</searchName>
      <title>Compliance Average Over All Fixlets (last 30 days)</title>
      <option name="charting.chart">area</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.axisY.maximumNumber">100</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>compliance by site over time</searchName>
      <title>Compliance Average Over All Fixlets By Site (last 30 days)</title>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.axisY.maximumNumber">100</option>
    </chart>
  </row>
  <row>
    <table>
      <searchName>compliance critical noncomp</searchName>
      <title>Top 25 Non-Compliant Critical Fixlets</title>
      <option name="count">10</option>
      <option name="showPager">true</option>
    </table>
  </row>
  <row>
    <chart>
      <searchName>noncompliance by cve</searchName>
      <title>Non-Compliance By CVE</title>
      <option name="charting.chart">column</option>
      <option name="charting.axisY.maximumNumber">100</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>noncompliance by cce</searchName>
      <title>Non-Compliance By CCE</title>
      <option name="charting.chart">column</option>
      <option name="charting.axisY.maximumNumber">100</option>
    </chart>
  </row>
</dashboard>
